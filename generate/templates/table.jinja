<div class='table-container {{table.units}}-units' v-if="{{table.units}}_visible">
 <div class='{{meta.table_fixed_header}}-container'>
    <table class="data-table table is-striped is-narrow is-hoverable is-fullwidth">
        <thead class="{{meta.table_fixed_header}}-thead">
            {% for heading in table.headings %} 
                <tr>
                    {%for header in heading.data %}
                        <th class="{{meta.table_fixed_header}}-header {{header.type}}" scope="col"> {{header.data}}</th>
                    {% endfor %}
                </tr>
            {% endfor %}
        </thead>
        <tbody>
            {% for row in table.rows %} 
                <tr>
                    {%for column in row.data %}
                        <td class="{{column.type}}"> {{column.data}}</td>
                    {% endfor %}
                </tr>
            {% endfor %}
        </tbody>
    </table>
 </div>
</div>