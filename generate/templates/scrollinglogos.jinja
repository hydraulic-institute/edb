
<div class="mt-4 container">
  <hr>
  <span class="mb-2" style="{{data.title_style}}">{{data.title }}</span>
  <div class="logos-wrapper">
    {% set logos_len = logos|length %}
    {% for logo in logos %}
      <div class="logo-item" style="left: max(calc(200px * {{logos_len}}), 100%); animation-delay: calc(30s / {{logos_len}} * ({{ logos_len }} - {{loop.index}}) * -1);">
        <img src="{{logo}}"/>
      </div>
    {% endfor %}
  </div>
  <span class="mt-2" style="{{data.footer_style}}">{{ data.footer }}</span>
  <hr>
</div>
