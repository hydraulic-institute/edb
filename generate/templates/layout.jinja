<script type="text/x-template" id="viscosity-converter-template">
    <div>
        <div class="row">
            <div class="col align-right">
                <button type="button" class="button is-small is-link" style="float:right" @click='swap()'>Swap</button> 
            </div>
        </div>
        <div class="row"> 
        <form>
            <div class='field'>
                <span class='label'>
                    <span>Convert from</span>
                </span>
                <div class='columns'>
                    <div class='column'>
                        <input class='input' v-model='from_value'/>
                    </div>
                    <div class='column'>
                        <select  class="input" v-model='from_unit'>
                            <option :value='u' v-for='u in units.filter(function(u){return u.no_input != true;})' :key='u.id' v-html='u.label'></option>
                        </select>
                    </div>
                </div>
            </div>
            <div class='field'>  
                <span class='label'>
                    <span>To</span>
                </span>
                <div class='columns'>
                    <div class='column'>
                        <select  class="input" v-model='to_unit'>
                            <option :value='u' v-for='u in units' :key='u.id' v-html='u.label'></option>
                        </select>                   
                    </div>
                    <div class='column'    v-if='show_sg'>
                        <div class="columns">
                            <div class='column'>
                                <span class='label'>
                                    <span>w/ SG</span>
                                </span>
                            </div>
                            <div class='column'>
                                <input class='input' v-model='sg'/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="field" v-if='to_unit'>
                <div class='column has-background-info has-text-weight-semibold' style='margin-bottom:1rem'>
                    <span class='has-text-warning' style='margin-right:0.1em;'>${from_value} <span v-html='from_unit.label' /></span>
                    <span class='has-text-warning' style='margin-right:0.1em;' v-if='show_sg'>with ${sg} SG</span>
                    <span class='has-text-white-bis' style='margin-right:0.1em;'>is equal to ${to_value} <span v-html='to_unit.label' /></span>
                    <p v-if='kinematic_warning' class='has-text-danger'>${kinematic_warning}</p>
                </div>
            </div>
        </form>
        </div>
        <div v-if='steps.length'>
            <button type='button' class="button is-small is-link is-light" v-if='show_steps' @click='show_steps=false'>Hide Steps</button>
            <button type='button' class="button is-small is-link is-light" v-else @click='show_steps=true'>Show Steps</button>
            <div style='padding-top:1rem;' v-if='show_steps'>
                <pre v-for="step in steps" v-html='step' />
            </div>
        </div>
    </div>
</script>


<script type="text/x-template" id="converter-template">
    <form>
        <div class='columns'>
            <div class='column'>
                <nav class="panel">
                    <p class="panel-heading" style='margin-bottom:0px'>Measures </p>
                    <a :class='{isActive: u == unit}' v-for='u in units' class='panel-block' @click='unit = u'>
                        <span class='panel-icon'>
                            <img :src='"./images/"+u.image'/>
                        </span>
                        <span>${u.measure}</span>
                    </a>
                </nav>
            </div>
            <div class='column'>
                <nav class="panel" v-if='unit'>
                <button @click='swap' type="button" style="float:right;margin-right:1rem;margin-top:0.5rem" class="button is-small is-link is-light">Swap</button>
                    
                    <p class="panel-heading" style='margin-bottom:0px'>${unit.measure}</p>
                    <div class='panel-block'>
                        
                        <div class='field'>
                        
                           <span class='label'>
                                <span>From</span>
                                
                            </span>
                            <div class='columns'>
                                <div class='column'>
                                    <input class='input' v-model='from'/>
                                </div>
                                <div class='column'>
                                    <select  class="input" v-model='unit_from'>
                                        <option :value='u' v-for='u in unit.units' v-html='u.label'></option>
                                    </select>
                                </div>
                            </div>
                        </div>
                      </div>
                    <div class='panel-block'>
                        <div class='field'>
                            <span class='label'>
                                <span>To</span>
                                
                            </span>
                            <div class='columns'>
                                <div class='column'>
                                    <input class='input' v-model='to' readonly disabled/>
                                </div>
                                <div class='column'>
                                    <select  class="input" v-model='unit_to'>
                                        <option :value='u' v-for='u in unit.units' v-html='u.label'></option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </form>
</script>

<script type="text/x-template" id="friction-loss-calculator-template">
	<form>
        <div class='field'>
                    <span class='label'>Select Material</span>
                    <select  class="input" v-model='material'>
                        <option value=''></option>
                        <option :value='m' v-for='m in materials'>${m}</option>
                    </select>

                </div>
        <div class='columns'>
            <div class='column'>
                
                <div class='field' v-if='sizes && sizes.length'>
                    <span class='label'>Select Nominal Size (in)</span>
                    <select class='input'v-model='nominal_size'>
                        <option value=''></option>
                        <option :value='m' v-for='m in sizes'>${m}</option>
                    </select>
                </div>
                <div class='field' v-if='selector'>
                    <span class='label'>Select ${selector}</span>
                    <select class='input' v-model='selection'>
                        <option value=''></option>
                        <option :value='m' v-for='m in selectors'>${m}</option>
                    </select>
                </div>
                <hr/>
                <div v-if='specified'>
                    <div class='field'>
                        <span class='label'>Nominal Outside Diameter (${units_length_short})</span>
                        <span> ${value_length_short(outer_diameter)}</span>
                    </div>
                    <div class='field'>
                        <span class='label'>Inside Diameter (${units_length_short})</span>
                        <span> ${value_length_short(inner_diameter)}</span>
                    </div>
                    <div class='field'>
                        <span class='label'>Absolute roughness (epsilon)</span>
                        <span> ${value_length_ft_mm(epsilon, 7)} ${units_length_ftmm}</span>
                    </div>
                    <div class='field'>
                        <span class='label'>Relative roughness (epsilon/D)</span>
                        <span> ${(epsilon/(inner_diameter/12)).toFixed(5)}</span>
                    </div>
                </div>
            </div>
        
            <div class='column'>
            <div v-if='entry'>
                <div class='field'>
                    <span class='label' >Flow Rate (${units_flowrate})</span>
                    <input class='input' type='number' @keydown='no_negative' min="0" step='0.01' v-model='flow'/>
                </div>
                <div class='field'>
                    <span class='label' >Pipe Length (${units_length_long})</span>
                    <input class='input' type='number' @keydown='no_negative' min="0" step='0.01' v-model='length'/>
                </div>
                <hr/>
                <div class='field'>
                    <span class='label' >Viscosity</span>
                    <div class='control'>
                        
                        <span class='radio'>
                            <input type='radio' name = 'viscosity_mode' @keydown='no_negative' min="0" value='kinematic' v-model='vka'/>
                            Kinematic
                        </span>
                        <span class='radio'>
                            <input type='radio' name = 'viscosity_mode' value='absolute' v-model='vka'/>
                            Dynamic (absolute)
                        </span>
                    </div>
                </div>
                <div class='field'>
                    <span class='label'>Kinematic Viscosity (cSt)</span>
                    <input class='input' :disabled="vka == 'absolute'" type='number' @keydown='no_negative' min="0" step='0.01' v-model='input_kinematic_v'/>
                </div>
                
                <div class='field'>
                    <span class='label'>Dynamic (absolute) Viscosity (cP)</span>
                    <input class='input' :disabled="vka != 'absolute'" type='number' @keydown='no_negative' min="0" step='0.01' v-model='input_dynamic_v'/>
                </div>
                
                <div class='field'>
                    <span class='label'>Specific Gravity</span>
                    <input class='input' :disabled="vka != 'absolute'" type='number' @keydown='no_negative' min="0" step='0.01' v-model='sg'/>
                </div>
           </div>
         </div>
         <hr/>
        </div>
         <div v-if='entry && results_revision.length'>
            <table>
                <thead>
                    <tr>
                        <th style="text-align:center">Flowrate</th>
                        <th style="text-align:center">Fluid Velocity</th>
                        <th style="text-align:center">Velocity Head</th>
                        <th style="text-align:center">Reynolds Number</th>
                        <th style="text-align:center">Flow Regime</th>
                        <th style="text-align:center">Friction Factor</th>
                        <th style="text-align:center">Head Loss</th>
                    </tr>
                     <tr>
                        <th style="text-align:center">(${units_flowrate})</th>
                        <th style="text-align:center"><b>V</b>&nbsp;&nbsp;(${units_velocity})</th>
                        <th style="text-align:center"><b>V<sup>2</sup>/2g</b>&nbsp;&nbsp;(${units_length_long})</th>
                        <th style="text-align:center"><b>Re</b></th>
                        <th style="text-align:center"></th>
                        <th style="text-align:center">f</th>
                        <th style="text-align:center">h<sub>f</sub> (${units_length_long})</th>
                    </tr>
                </thead>
                <tbody>
                    <tr v-for='sample in results_revision' :class="{reference_flow: sample.reference}">
                        <td style="text-align:center"><unit-value :value="value_flowrate(sample.flow)"/></td>
                        <td style="text-align:center"><unit-value :value="value_velocity(sample.velocity)"/></td>
                        <td style="text-align:center"><unit-value :value="value_length_long(sample.velocity * sample.velocity / (2 * 32.17))"/></td>
                        <td style="text-align:center">${sample.reynolds}</td>
                        <td style="text-align:center">
                            <span v-if='sample.laminar'>Laminar</span>
                            <span v-else>Turbulent</span>
                        </td>
                        <td style="text-align:center">
                            <span><unit-value :value="sample.friction_loss"/></span>
                        </td>
                        <td style="text-align:center">
                            <span><unit-value :value="value_length_long(sample.head_loss)"/></span>
                        </td>
                    </tr>
                </tbody>
            </table>
         </div>

    </form>
</script>

<script type="text/x-template" id="mechanical-friction-loss-calculator-template">
    <div>
        <form>
            <div class='columns'>
                <div class='column'>
                    <div class='columns'>
                        <div class='column'>
                            <div class='field'>
                                <span class='label'>
                                    <span>Shaft Diameter [D] (${shaft_min}-${shaft_max} ${units_measure_in_mm})</span>
                                </span>
                                <div>
                                    <input class='input' type='number' @keydown='no_negative' v-model='shaft_diameter'/>
                                    <span v-if='msg.shaft' style="color:red; font-size:smaller"><i>${msg.shaft}</i></span>
                                    <span v-else>&nbsp;</span>
                                </div>
                            </div>
                            <div class='field'>
                                <span class='label'>
                                    <span>Speed (${rpm_min}-${rpm_max} RPM)</span>
                                </span>
                                <div>
                                    <input class='input' type='number' @keydown='no_negative' v-model='rpm_value'/>
                                    <span v-if='msg.rpm' style="color:red; font-size:smaller"><i>${msg.rpm}</i></span>
                                    <span v-else>&nbsp;</span>
                                </div>
                            </div>
                        </div>
                        <div class='column'>
                            <div class='field'>
                                <span class='label' >Pump Setting Length [L] (${units_measure_ft_m})</span>
                                <input class='input' type='number' @keydown='no_negative' min="0" step="5" v-model='length'/>
                                <span>&nbsp;</span>
                            </div>
                            <div>
                                <span class="label" for='bearing_toggle'>Enclosed shaft with flooded tube</span>
                                <input type="checkbox" id='bearing_toggle' v-model='bearing_spacing_5'/>
                            </div>
                        </div>
                    </div>
                    <div class='mt-2'>
                    </div>
                    <div class='mt-4'>
                        <div class="field">
                            <div class='column has-background-info has-text-weight-semibold' style='margin-bottom:1rem'>
                                <span class='has-text-warning' style='margin-right:0.1em;'>Mechanical Friction Loss (${units_friction_loss}): </span>
                                <span v-bind:class = "mech_friction_loss?'has-text-white-bis':'has-text-danger'">${mech_friction_loss} ${units_friction_loss}</span>
                            </div>
                        </div>
                        <div class="field">
                            <div class='column has-background-info has-text-weight-semibold' style='margin-bottom:1rem'>
                                <span class='has-text-warning' style='margin-right:0.1em;'>Mechanical Friction Loss (${units_friction}): </span>
                                <span v-bind:class = "mech_friction?'has-text-white-bis':'has-text-danger'">${mech_friction} ${units_friction}</span>
                            </div>
                        </div>
                    </div> 
                </div>    
                <div class='column' align='center'>
                    <img alt='Hydraulic Institute' src="./images/mechanical-friction-loss-calculator-pump.jpg" style="max-height:100vh">     
                </div>
            </div>
        </form>
    </div>
</script>

<script type="text/x-template" id="atmospheric-pressure-calculator-template">
    <div class="wrap">
        <div>
            Instruction:  Enter an elevation above sea level to see the standard atmospheric pressure difference.<br>  
            Elevation is limited to 36,000 feet or 10,900 meters.
        </div>
        <div class='container mt-5'>
            <div class='field'>
                <h5>Elevation above sea level (${units_measure_ft_m})</h5>
                <div class="row">
                    <div class="col-3">
                        <input sytle="width:40%" class='input' type='number' min='0' @keydown='no_negative' v-model.number='elevation'>
                    </div>
                    <div class="col">
                        <span v-if='err_msg' style="color:red; font-size:smaller"><i>${err_msg}</i></span>
                        <span v-else>&nbsp;</span>
                    </div>
                </div>
            </div>
            <table class="table is-narrow border border-secondary-subtle" :key="entry">
                <thead class="fixed-thead">
                    <th style="width:40%; ">Units</th>
                    <th style="width:20%;">At Elevation</th>
                    <th style="width:20%;">At Sea Level (std)</th>
                    <th style="width:20%;">Delta</th>
                </thead>
                <tbody>
                    <tr class="hi_no_input" v-for='d in calc_data'>
                        <td><b>${d.desc}</b></td>
                        <td>${d.val}</td>
                        <td>${d.sea_level}</td>
                        <td>${d.delta}</td>
                    </tr>
                </tbody>
            </table>
        </div>
        <br>
        <br>
        <b>*</b>Values do not include local variations due to weather, etc.<br><br>
        For other pressures, convert <a href="/tools/unit-conversions.html">HERE</a>
    </div>
</script>

<script type="text/x-template" id="tank-demo-template"> 
    <div class="wrap">  
        <div class="row mb-4">
            <div class="columns">
                <div class="col-8">
                    <div class="row mt-2">
                        <div class="col">
                        <div class="row mt-2 mb-2">
                            <div class="col-4">Tank Orientation</div>
                            <div class="col-4">${tank_type}</div>
                        </div>
                        <div v-for='c in volume_strings'>
                            <div v-if='c.type' class="row align-items-center">
                                <div class="col-4">
                                <b>${c.type}</b>
                                </div>
                                <div class="col-4">
                                    <select  class="input" style="min-width:120px;" v-model="$data[c.varname]">
                                        <option :value='b' v-for='b in end_types'>${b}</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row align-items-center">
                            <div class="col-4"><b>Length Units</b></div>
                            <div class="col-4">
                                <select  class="input" style="min-width:120px;" v-model='length_unit'>
                                    <option :value='l' v-for='l in length_types'>${l}</option>
                                </select>
                            </div>
                        </div>
                        <div class="row align-items-center">
                            <div class="col-4">
                                <b>Tank Diameter: [D]</b>  
                            </div>
